<table>
  <tbody>

    <tr>
      <th>
        Ranking
      </th>
      <th>
        Name
      </th>
      <th>
        Upvote
      </th>
    </tr>

    <% @albums.each do |m| %>
    <tr>
      <td>
        Ranked: <%= m.rank %>
      </td>
      <td>
        <%= m.title %>
      </td>
      <td>
        <%= button_to "Upvote", update_album_path(m.id), method: :patch%>
      </td>
    </tr>
    <% end %>
  </tbody>
</table>


<%= link_to "View All Media", root_path%>
<%= link_to "Create New Album", new_album_path %>
